<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="ReportPortal" parallel="tests" thread-count="3">
    <listeners>
        <listener class-name="com.epam.gomel.taf.framework.utils.TestListener"/>
    </listeners>

    <test name="ReportPortal_parallel_test_1">
        <classes>
            <class name="com.epam.gomel.taf.test.ReportPortalTest">
            </class>
        </classes>
    </test>

    <test name="ReportPortal_parallel_test_2">
        <classes>
            <class name="com.epam.gomel.taf.test.SmokeTest">
            </class>
        </classes>
    </test>

    <test name="ReportPortal_parallel_test_3" junit="true">
        <classes>
            <class name="com.epam.gomel.taf.test.LaunchesTest">
            </class>
        </classes>
    </test>
</suite>
